<?xml version="1.0" encoding="utf-8"?>
<actions>
   <action intentName="actions.intent.ADD_ITEM">
      <fulfillment urlTemplate="ultrachango://add_item?name={name}&amp;list={list}">
         <parameter-mapping intentParameter="name" urlParameter="name"/>
         <parameter-mapping intentParameter="list" urlParameter="list"/>
      </fulfillment>
   </action>
</actions>